{
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },

  "parserOptions": {
    "sourceType": "module"
  },

  "rules": {
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "quotes": ["error", "double"],
    "no-console": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
